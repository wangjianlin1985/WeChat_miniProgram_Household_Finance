<view class="container">
  <loading hidden="{{loadingHide}}">
    {{loadingText}}
  </loading>

  <view class="title">收入类型详情</view>
  <view class="incomeType_detail_view"> 
      <view class="viewItem">
        <view class="label">收入类别id：</view>
        <view class="content">
          <view>{{incomeTypeId}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">收入类别名称：</view>
        <view class="content"> 
          <view>{{incomeTypeName}}</view>
        </view>
      </view>

      <view>
        <navigator class="btnReturn" open-type="navigateBack" hover-class="navigator-hover">返回</navigator>
      </view>

  </view>
</view>

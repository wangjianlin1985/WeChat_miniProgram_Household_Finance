<view class="container">
  <loading hidden="{{loadingHide}}">
    {{loadingText}}
  </loading>
  <view class="title">添加图收入类型</view>

  <view class="incomeType_add_view">
    <form bindsubmit="formSubmit">
      <view class="formItem">
        <view class="label">收入类别名称：</view>
        <view class="formControl">
          <input type="text" name="incomeType.incomeTypeName" placeholder="请输入收入类别名称" value="{{incomeTypeName}}" />
        </view>
      </view>

      <view>
        <button class="vBtn_ok" formType="submit" disabled="{{disabled}}">提交发布</button>
      </view>

    </form>
  </view>
</view>
